{
  "name": "nl2page",
  "version": "1.0.0",
  "description": "Natural Language to Web Page Converter",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm start",
    "client": "cd client && npm start",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "keywords": [
    "natural-language",
    "web-page",
    "openai",
    "gpt"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "archiver": "^7.0.1",
    "concurrently": "^8.2.2"
  }
}
